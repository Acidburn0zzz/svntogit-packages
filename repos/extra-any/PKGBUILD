package_thunderbird-i18n-ar() {
  pkgdesc="Arabic language pack for Thunderbird"
  cd ${srcdir}
  install -Dm644 thunderbird-i18n-$pkgver-ar.xpi "${pkgdir}/$_path/langpack-ar@thunderbird.mozilla.org.xpi"
}

